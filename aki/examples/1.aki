# Comment example

def g1 (z1=0) {
    var q=z1
    loop (var x=1,x<20, x+1) {
        q+=1
    }
    q
}

# Another comment example

def g2 (z2,z3=0) {
    loop (var x=1, x<20, x+1) {
        z2+=1
    }    
    z2
}

# A third comment example

def g3(z4){
    loop () {
        when z4>=20 break
        z4+=1        
    }
    z4
}

